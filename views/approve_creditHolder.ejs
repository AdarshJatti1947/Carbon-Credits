              }
              else
              {
                  alert(resp.data.message);
              }
            }
          );
        })


        $("#approve").click(function (){
            $.post(
            "http://localhost:3000/creditHolder/confirm-purchase",
            {
              purchaseId: document.getElementById("orderId").value,
            },
            async function (resp, status, xhr) {
              if(resp.status == 200)
              {
                  let permit = await pay(resp.data.transferDetails);
                  verify(permit,resp.data.transferDetails.id)
              }
              else
              {
                  alert(resp.data.message);
              }
            }
          );
        })

        
    })

    async function pay(paymentInfo)
        {
            try
            {
                if(typeof window.ethereum !== 'undefined')
                {
                    await window.ethereum.request({method:"eth_requestAccounts"});
                    let provider = new ethers.providers.Web3Provider(window.ethereum);
                    let sign = provider.getSigner();
                    let users_contract = new ethers.Contract(token_contract_address,abi.abi,sign);

                    let spender = paymentInfo.pkaddress;
                    let amount = ""+paymentInfo.amount;
                    amount =  ethers.utils.parseEther(amount);

                    const transaction = await  users_contract.approve(spender,amount);

                    const receipt = await transaction.wait();
                    console.log(receipt);
                    return true;
                }
                else
                {
                    return false;
                }

            }
            catch(err)
            {
                console.log(err);
                return false;
            }
        }

        function verify(permit,purchaseInfo)
        {
            if(permit)
            {
                $.post(
                    "http://localhost:3000/creditHolder/finalize-approval",
                    {
                    purchaseId: purchaseInfo,
                    },
                    function (resp, status, xhr) {
                    if(resp.status == 200)
                    {
                        alert(resp.data.message);
                    }
                    else
                    {
                        alert(resp.data.message);
                    }
                    }
                );
            }
            else
            {
                alert("some issue has occured");
            }
        }

        function show_projects(requests)
        {

            let tablediv = document.getElementsByClassName("test1"); 
            tablediv[0].innerHTML = "";

            let headers = document.createElement("thead");

            let row = document.createElement("tr");

            let title1 = document.createElement("th");
            title1.innerHTML = "ID";

            let title2 = document.createElement("th");
            title2.innerHTML = "Amount";

            let title3 = document.createElement("th");
            title3.innerHTML = "Project ID";

            let title4 = document.createElement("th");
            title4.innerHTML = "Project Name";

            let title5 = document.createElement("th");
            title5.innerHTML = "Seller Public Key Address";

            let title6 = document.createElement("th");
            title6.innerHTML = "Official contact";

            row.appendChild(title1);
            row.appendChild(title2);
            row.appendChild(title3);
            row.appendChild(title4);
            row.appendChild(title5);
            row.appendChild(title6);

            headers.appendChild(row);
            tablediv[0].appendChild(headers);
            for(i=0;i<requests.length;i++)
            {
                let user = document.createElement("tr");
                user.classList.add("userMsg");

                let Id = document.createElement("th");
                Id.innerHTML= requests[i].id
                Id.classList.add("text1");

                let Amount = document.createElement("th");
                Amount.innerHTML= requests[i].amount
                Amount.classList.add("text1");

                let Project_Id = document.createElement("th");
                Project_Id.innerHTML= requests[i].projectId
                Project_Id.classList.add("description");

                let Project_Name = document.createElement("th");
                Project_Name.innerHTML= requests[i].projectName
                Project_Name.classList.add("description");

                let Seller_Public_Key_Address = document.createElement("th");
                Seller_Public_Key_Address.innerHTML= requests[i].customerPKA
                Seller_Public_Key_Address.classList.add("description");

                let Official_email_address = document.createElement("th");
                Official_email_address.innerHTML= requests[i].officialEmail
                Official_email_address.classList.add("description");

                

                let tablediv = document.getElementsByClassName("test1"); 
                
                user.appendChild(Id);
                user.appendChild(Amount);
                user.appendChild(Project_Id);
                user.appendChild(Project_Name);
                user.appendChild(Seller_Public_Key_Address);
                user.appendChild(Official_email_address);
                
                tablediv[0].appendChild(user);
                ind1 = 1;


